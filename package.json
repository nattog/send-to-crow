{
	"name": "send-to-crow",
	"publisher": "nattog",
	"displayName": "Send To Crow",
	"description": "Send lines of text to Monome Crow",
	"version": "1.0.0",
	"license": "MIT",
	"engines": {
		"vscode": "^1.59.0"
	},
	"repository": {
		"url": "https://github.com/midouest/norns-repl"
	},
	"categories": [
		"Other"
	],
	"main": "./dist/extension.js",
	"keywords": [
		"Monome",
		"Crow",
		"Druid",
		"Sending text",
		"Web sockets"
	],
	"activationEvents": [
		"onCommand:sendToCrow.toggleActiveKeybindings",
		"onCommand:sendToCrow.sendSelection"
	],
	"contributes": {
		"commands": [
			{
				"command": "sendToCrow.toggleActiveKeybindings",
				"title": "Send To Crow: Toggle keybindings"
			},
			{
				"command": "sendToCrow.sendSelection",
				"title": "Send To Crow: Send selected text to crow"
			}
		],
		"keybindings": [
			{
				"command": "sendToCrow.sendSelection",
				"key": "ctrl+enter",
				"mac": "cmd+enter",
				"when": "editorLangId == lua && sendToCrow:isActive"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.59.0",
		"@types/ws": "^7.4.7",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"ts-loader": "^9.2.5",
		"ts-node": "^10.2.1",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.51.1",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"ws": "^8.2.1"
	}
}
